{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "UUID": {
      "type": "customField",
      "options": {
        "uuid-format": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
        "disable-regenerate": true
      },
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "description": {
      "type": "text"
    },
    "isCompany": {
      "type": "boolean",
      "default": false
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "isVerified": {
      "type": "boolean",
      "default": false
    },
    "userInfo": {
      "type": "component",
      "repeatable": false,
      "component": "informations.user-info"
    },
    "companyInfo": {
      "type": "component",
      "repeatable": false,
      "component": "informations.company-info"
    },
    "CNI": {
      "type": "component",
      "repeatable": false,
      "component": "informations.cni"
    },
    "SST": {
      "type": "component",
      "repeatable": false,
      "component": "informations.sst"
    },
    "KBIS": {
      "type": "component",
      "repeatable": false,
      "component": "informations.kbis"
    },
    "CQP": {
      "type": "component",
      "repeatable": false,
      "component": "informations.cqp"
    },
    "ProCard": {
      "type": "component",
      "repeatable": false,
      "component": "informations.pro-card"
    },
    "departments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::department.department",
      "mappedBy": "users"
    },
    "professions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::profession.profession",
      "mappedBy": "candidates"
    },
    "offers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::job.job",
      "mappedBy": "company"
    },
    "jobs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::job.job",
      "mappedBy": "candidates"
    },
    "favorites": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::job.job",
      "mappedBy": "wishes"
    },
    "isDeleted": {
      "type": "boolean",
      "default": false
    }
  }
}
